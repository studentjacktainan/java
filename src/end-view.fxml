<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views">
   <children>
      <Label layoutX="172.0" layoutY="30.0" text="遊戲結束">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <Label layoutX="228.0" layoutY="139.0" text="感謝您的遊玩">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="viewRank" layoutX="88.0" layoutY="245.0" mnemonicParsing="false" onAction="#viewRankClicked" text="查看排行榜">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="returnToMain" layoutX="258.0" layoutY="245.0" mnemonicParsing="false" onAction="#returnToMainClicked" text="回到主畫面">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="422.0" layoutY="248.0" mnemonicParsing="false" onAction="#exitClicked" text="離開">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
